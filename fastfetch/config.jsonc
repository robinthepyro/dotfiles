// // Inspired by Catnap
{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small",
        "padding": {
            "top": 4,
            "left":3,

        }
    },
    "display": {
        "separator": " "
    },
    "modules": [
        {
            "key": "╭────────────╮",
            "type": "custom"
        },
        {
            "key": "│ {#31} user    {#keys} │",
            "type": "title",
            "format": "{user-name}"
        },
        {
            "key": "│ {#32}󰇅 hostname{#keys} │",
            "type": "title",
            "format": "{host-name}"
        },
        {
            "key": "│ {#33}󰅐 uptime  {#keys} │",
            "type": "uptime"
        },
        {
            "key": "│ {#34}󰩟 network {#keys} │",
            "type": "localip",
            "format": "{ipv4} ({ifname})"
        },
        {
            "key": "│ {#35}{icon} distro  {#keys} │",
            "type": "os"
        },
        {
            "key": "│ {#36} kernel  {#keys} │",
            "type": "kernel"
        },
        {
            "key": "│ {#31}󰇄 wm      {#keys} │",
            "type": "wm"
        },
        {
            "key": "│ {#32} packages{#keys} │",
            "type": "packages"
        },
        {
            "key": "│ {#33} shell   {#keys} │",
            "type": "shell"
        },
        {
            "key": "│............│",
            "type": "custom"
        },
        {
            "key": "│ {#34}󰍛 cpu     {#keys} │",
            "type": "cpu",
            "showPeCoreCount": true
        },
        {
            "key": "│ {#35}󰉉 disk    {#keys} │",
            "type": "disk",
            "folders": "/"
        },
        {
            "key": "│ {#36} memory  {#keys} │",
            "type": "memory"
        },
        {
            "key": "╰────────────╯",
            "type": "custom"
        }
    ]
}

